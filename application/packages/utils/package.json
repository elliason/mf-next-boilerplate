{
  "name": "@mf-project/utils",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,css,scss,less,json,yml,yaml.md,mdx}\"",
    "lint": "pnpm run lint:scripts:fix && pnpm run lint:ts",
    "lint:scripts": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs .",
    "lint:scripts:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs --fix .",
    "lint:ts": "tsc --noEmit",
    "test": "jest",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist/*"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@mf-project/eslint-config": "workspace:^",
    "@mf-project/prettier-config": "workspace:^",
    "@mf-project/stylelint-config": "workspace:^",
    "@mf-project/typescript-config": "workspace:^",
    "@total-typescript/ts-reset": "^0.3.7",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}
